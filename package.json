{
  "name": "loglify",
  "version": "1.0.0",
  "description": "A Probot app",
  "author": "Brian 'bdougie' Douglas <ilikerobot@gmail.com>",
  "license": "ISC",
  "repository": "https://github.com/bdougie/loglify.git",
  "homepage": "https://github.com/bdougie/loglify",
  "bugs": "https://github.com/bdougie/loglify/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@probot/serverless-lambda": "^0.4.0",
    "probot": "^9.11.2"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "nodemon": "^2.0.1",
    "smee-client": "^1.1.0",
    "standard": "^10.0.3"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  }
}
